{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DE-ADF-DEV-APAC"
		},
		"LS_ADB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ADB'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_ADB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nag-abila01.database.windows.net"
		},
		"LS_ADB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-dev_apac"
		},
		"LS_ADB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "abadmin"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devapac1storage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DE-ADF-DEV-APAC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADB_password": {
						"value": "[parameters('LS_ADB_password')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_ADB_properties_typeProperties_server": {
						"value": "[parameters('LS_ADB_properties_typeProperties_server')]"
					},
					"LS_ADB_properties_typeProperties_database": {
						"value": "[parameters('LS_ADB_properties_typeProperties_database')]"
					},
					"LS_ADB_properties_typeProperties_userName": {
						"value": "[parameters('LS_ADB_properties_typeProperties_userName')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DE-ADF-DEV-APAC_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DE-ADF-DEV-APAC_ArmTemplate_0')]"
			]
		}
	]
}